{
    "name": "symfgenus/mpdf-wrapper",
    "type": "symfony-bundle",
    "description": "This bundle allows to use mpdf library with symfony3.",
    "keywords": ["Symfony2", "Symfony3", "bundle", "mpdf", "wrapper"],
    "homepage": "https://github.com/hanishsingla/mpdf-wrapper",
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Hanish Singla",
            "email": "hksingla92@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "~3.0|~4.0",
        "mpdf/mpdf": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Symfgenus\\MpdfWrapper\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfgenus\\MpdfWrapper\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x.x-dev"
        }
    }
}
